<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="PolyCameraTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="ToolbarStyle" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        <!--Any text styling can be done here-->
        <item name="android:fontFamily">sans-serif-light</item>
        <item name="titleTextAppearance">@style/TextView.LightFont.NormalStyle.White.Medium</item>
        <item name="subtitleTextAppearance">@style/TextView.LightFont.NormalStyle.White.Small</item>
    </style>

    <style name="Theme.MaterialComponents.Light.ActionBar" parent="@style/Theme.MaterialComponents.Light">
        <item name="android:windowNoTitle">false</item>
        <item name="windowActionBar">true</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:toolbarStyle">?attr/toolbarStyle</item>
        <item name="android:statusBarColor">@color/primary</item>
    </style>

    <style name="DefaultTextView" parent="android:Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:paddingLeft">2dp</item>
        <item name="android:paddingRight">2dp</item>
    </style>

    <style name="EditTextStyle" parent="Widget.AppCompat.EditText">
        <item name="android:colorControlNormal">@color/black_almost</item>
        <item name="android:textColor">@color/black_main</item>
        <item name="android:textColorHint">@color/black_light</item>
        <item name="android:layout_width">wrap_content</item>
