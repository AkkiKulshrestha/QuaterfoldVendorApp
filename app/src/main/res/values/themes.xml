<resources>
    <!-- Base application theme. -->
    <style name="Theme.QuaterfoldVendorApp" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryDark">@color/primary</item>
        <item name="colorAccent">@color/secondary</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.QuaterfoldVendorApp.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.QuaterfoldVendorApp.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.QuaterfoldVendorApp.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>